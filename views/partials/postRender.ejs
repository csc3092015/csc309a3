<div class="">
	<div class="post">
		<h2 class="post-title">
			<%= postBO.getTitle();%>
		</h2>
		<!--
		<p>
			PostId: <%= postBO.getPostId();%>
		</p>-->
		<div class="keyValuePair">
			<div class="post-label">
				<p>Keywords: </p>
			</div>
			<div class="post-field">
				<p>
					<%= postBO.getKeywordsArray();%>
				</p>
			</div>
		</div>

		<div class="keyValuePair">
			<div class="post-label">
				<p>Description: </p>
			</div>
			<div class="post-field">
				<p>
					<%= postBO.getDescription();%>
				</p>
			</div>
		</div>

		<div class="post-metaData">
		<p>Provider: <%= postBO.getAuthorId();%></p>
		<!--
		<p>
			ByWho: <%= postBO.getByWho();%>
		</p>
		<p>
			IsPurchased: <%= postBO.getIsPurchased();%>
		</p>
		-->
		<p>Posted On: <%= postBO.getCreatedAt();%></p>
		<p>Expires On: <%= postBO.getIsExpired();%></p>
		</div>
	</div>
</div>