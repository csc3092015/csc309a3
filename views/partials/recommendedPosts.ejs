<% if(typeof postBOByConsumerArray != 'undefined'){ %>
	<div class="well"> 
		<h1>Here are some people may want Your Service!!</h1>
		<ul>
			<% postBOByConsumerArray.forEach(function(postBO) { %>
			<li>
				<% include ../partials/postRender.ejs %>
			</li>
			<% }); %>
		</ul>
	</div>
<% } if (typeof postBOByProviderArray != 'undefined'){ %> 
	<div class="well"> 
	   	<h1>Here are some people may provide what Your Want!!</h1>
	   	<ul>
			<% postBOByProviderArray.forEach(function(postBO) { %>
			<li>
				<% include ../partials/postRender.ejs %>
			</li>
			<% }); %>
		</ul>
	</div>
<% } if (typeof postBOFeaturedArray != 'undefined') {%>
	<div class="well"> 
   <h1>Here are just some random posts not yours!!</h1>
	   	<ul>
			<% postBOFeaturedArray.forEach(function(postBO) { %>
			<li>
				<% include ../partials/postRender.ejs %>
			</li>
			<% }); %>
		</ul> 
	</div>
<% } %>