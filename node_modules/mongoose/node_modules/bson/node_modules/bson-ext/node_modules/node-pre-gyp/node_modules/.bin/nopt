../nopt/bin/nopt.js
=======
#!/bin/sh
basedir=`dirname "$0"`

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../nopt/bin/nopt.js" "$@"
  ret=$?
else 
  node  "$basedir/../nopt/bin/nopt.js" "$@"
  ret=$?
fi
exit $ret
